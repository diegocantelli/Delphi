unit uClasseCliente;

interface

type
  TCliente = class
    private
      FId:Integer;
      FNome:string;
      FDtNasc:TDate;
    public
      properties Id:Integer read FId write FId;
      properties Nome: string read FNome write FNome;
      properties DtNasc:TDate read FDtNasc write FDtNasc;
  end;

implementation

end.
