unit uPessoa;

interface

type
  TPessoa = class
  private
    FPessoa: string;
    FCodigo: Integer;
  published
  public
    constructor Create;
    destructor Destroy;override;
    property Pessoa:string read FPessoa write FPessoa;
    property Codigo:Integer read FCodigo write FCodigo;
  end;

implementation

end.
